    "url": "https://github.com/mobily/re-date.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.2",
    "all-contributors-cli": "^5.4.1",
    "bs-platform": "^4.0.7",
    "coveralls": "^3.0.2",
    "markdown-contents": "^1.0.11",
    "ramda": "^0.26.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/__tests__/*_test.bs.js"
    ],
    "modulePathIgnorePatterns": [
